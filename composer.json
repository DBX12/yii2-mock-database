{
    "name": "dbx12/yii2-mock-database",
    "description": "Mock connection for the Yii2 database connection. This connection can be configured to fail operations for testing purposes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DBX12",
            "email": "dbx12@dbx12.de"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "yiisoft/yii2": "^2.0.49.4",
        "phpunit/phpunit": "9.5.8"
    },
    "autoload": {
        "psr-4": {
            "dbx12\\yii2MockDatabase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dbx12\\yii2MockDatabase\\tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://asset-packagist.org"
      }
    ]
}
